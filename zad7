#include <iostream>
using namespace std;
int main() {
    int a,b,nww;
    cout<<"Podaj 2 liczby: ";
    cin>>a>>b;
    if(a>0 && b>0){}else{nww=0;return 1;}
    int a1=a;
    int b1=b;
    while(a1!=b1){
        if(a1>b1){
            b1=b1+b;
        }else{a1=a1+a;}
    }
    nww=a1;
    cout<<nww;
    return 0;
}
